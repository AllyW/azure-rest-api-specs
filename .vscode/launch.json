{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha Tests",
      "program": "${workspaceFolder}/node_modules/mocha/bin/_mocha",
      "args": [
        "--timeout",
        "999999",
        "--colors",
        "${workspaceFolder}/test/syntax.js"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${file}"
    },
    {
      "name": "tsp compile aaz arize",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": ["compile", "${workspaceFolder}/specification/liftrarize/ArizeAi.ObservabilityEval.Management/main.tsp"],
      "runtimeExecutable": "tsp",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "outFiles": [
        "${workspaceFolder}/packages/*/dist/**/*.js",
        "${workspaceFolder}/packages/*/dist-dev/**/*.js",
        "${workspaceFolder}/core/packages/*/dist/**/*.js",
        "${workspaceFolder}/core/packages/*/dist-dev/**/*.js"
      ]
    },
    {
      "name": "tsp compile mongo autorest",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": [
        "compile",
        "${workspaceFolder}/specification/mongocluster/DocumentDB.MongoCluster.Management/main.tsp",
        "--config",
        "${workspaceFolder}/specification/mongocluster/DocumentDB.MongoCluster.Management/tspconfig-autorest.yaml"
      ],
      "runtimeExecutable": "tsp",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "outFiles": [
        "${workspaceFolder}/packages/*/dist/**/*.js",
        "${workspaceFolder}/packages/*/dist-dev/**/*.js",
        "${workspaceFolder}/core/packages/*/dist/**/*.js",
        "${workspaceFolder}/core/packages/*/dist-dev/**/*.js"
      ]
    },
    {
      "name": "tsp compile mongo aaz",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": [
        "compile",
        "${workspaceFolder}/specification/mongocluster/DocumentDB.MongoCluster.Management/main.tsp",
        "--config",
        "${workspaceFolder}/specification/mongocluster/DocumentDB.MongoCluster.Management/tspconfig-aaz.yaml"
      ],
      "runtimeExecutable": "tsp",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "outFiles": [
        "${workspaceFolder}/packages/*/dist/**/*.js",
        "${workspaceFolder}/packages/*/dist-dev/**/*.js",
        "${workspaceFolder}/core/packages/*/dist/**/*.js",
        "${workspaceFolder}/core/packages/*/dist-dev/**/*.js"
      ]
    },
    {
      "name": "tsp compile mongo openapi3",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "runtimeArgs": [
        "compile",
        "${workspaceFolder}/specification/mongocluster/DocumentDB.MongoCluster.Management/main.tsp",
        "--config",
        "${workspaceFolder}/specification/mongocluster/DocumentDB.MongoCluster.Management/tspconfig-openapi3.yaml"
      ],
      "runtimeExecutable": "tsp",
      "skipFiles": ["<node_internals>/**"],
      "type": "node",
      "outFiles": [
        "${workspaceFolder}/packages/*/dist/**/*.js",
        "${workspaceFolder}/packages/*/dist-dev/**/*.js",
        "${workspaceFolder}/core/packages/*/dist/**/*.js",
        "${workspaceFolder}/core/packages/*/dist-dev/**/*.js"
      ]
    }
  ]
}
